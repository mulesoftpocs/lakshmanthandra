<?xml version='1.0' encoding='UTF-8'?>
<types:mule xmlns:types="http://www.mulesoft.org/schema/mule/types">
  <types:catalog>
    <types:type name="inputcsv" format="csv">
      <types:shape format="weave" example="examples/db.csv"><![CDATA[%dw 2.0

type inputcsv = Array<{|  eid: String,   ename: String,   esalary: String|}>

]]></types:shape>
      <types:properties>
        <types:property name="headerLineNumber" value="0"/>
        <types:property name="header" value="true"/>
        <types:property name="isQuotedString" value="false"/>
      </types:properties>
    </types:type>
    <types:type name="outputcsv" format="json">
      <types:shape format="weave" example="examples/csv.json"><![CDATA[%dw 2.0

type outputcsv = Array<{|  eid: Number {"typeId": "int"},   ename: String,   esalary: Number {"typeId": "int"}|}> {"example": "[{\\\"eid\\\":4662,\\\"ename\\\":\\\"Siva\\\\\\\"\\\",\\\"esalary\\\":12000}]"}

]]></types:shape>
    </types:type>
    <types:type name="xmlcsv" format="xml">
      <types:example format="xml" element="Employees" location="examples/csv.xml"/>
    </types:type>
  </types:catalog>
  <types:enrichment select="#be780dc1-2fb9-4b27-bfe2-41f2b052e744">
    <types:processor-declaration>
      <types:input-event>
        <types:message>
          <types:payload type="inputcsv"/>
        </types:message>
      </types:input-event>
      <types:output-event>
        <types:message>
          <types:payload type="outputcsv"/>
        </types:message>
      </types:output-event>
    </types:processor-declaration>
  </types:enrichment>
  <types:enrichment select="#8b94f751-6ac2-49e2-a5b5-3a6816b83c4a">
    <types:processor-declaration>
      <types:output-event>
        <types:message>
          <types:payload type="xmlcsv"/>
        </types:message>
      </types:output-event>
    </types:processor-declaration>
  </types:enrichment>
</types:mule>